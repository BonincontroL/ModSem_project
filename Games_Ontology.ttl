@prefix : <http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/> .

<http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology> rdf:type owl:Ontology ;
                                                                             owl:imports <http://www.ontologydesignpatterns.org/cp/owl/bag.owl> ,
                                                                                         <http://www.ontologydesignpatterns.org/cp/owl/list.owl> ;
                                                                             <http://purl.org/dc/elements/1.1/creator> "Alberto Aiello" ,
                                                                                                                       "Lorenzo Bonincontro" ;
                                                                             <http://purl.org/dc/elements/1.1/date> "2025-01-13 21:40:23"@en ;
                                                                             <http://purl.org/dc/elements/1.1/title> "Games_Ontology"@en ;
                                                                             :vann:preferredNamespacePrefix "http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology"@en ;
                                                                             :vann:preferredNamespaceUri "Games_Ontology"@en ;
                                                                             rdfs:comment "Ontology focused on character interactions, narrative aspects and settings of video games"@en ;
                                                                             rdfs:label "Games_Ontology"@en ;
                                                                             owl:versionInfo "1.0"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/vann:preferredNamespacePrefix
:vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/vann:preferredNamespaceUri
:vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2002/07/owl#equivalentClass
owl:equivalentClass rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem
<http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem> owl:propertyChainAxiom ( <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem>
                                                                                        <http://www.ontologydesignpatterns.org/cp/owl/list.owl#nextItem>
                                                                                      ) ,
                                                                                      ( <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem>
                                                                                        <http://www.ontologydesignpatterns.org/cp/owl/list.owl#previousItem>
                                                                                      ) .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/AppearsIn
:AppearsIn rdf:type owl:ObjectProperty ;
           owl:inverseOf :hasCharacter ;
           rdfs:comment "relation from a character to a game"@en ;
           rdfs:label "AppearsIn"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/CompanyMakeSeries
:CompanyMakeSeries rdf:type owl:ObjectProperty ;
                   owl:inverseOf :SeriesDevBy ;
                   rdfs:domain :DevPublisher ;
                   rdfs:range :Series ;
                   rdfs:comment "relation from a Company to a Serie (es. Fifa)"@en ;
                   rdfs:label "CompanyMakeSeries"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/DLCof
:DLCof rdf:type owl:ObjectProperty ;
       owl:inverseOf :hasDLC ;
       rdfs:domain :DLC ;
       rdfs:range :Game ;
       rdfs:comment "relation from a Game to a DLC"@en ;
       rdfs:label "DLCof"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/DevBy
:DevBy rdf:type owl:ObjectProperty ;
       owl:inverseOf :makes ;
       rdfs:domain <http://schema.org/Product> ;
       rdfs:range :DevPublisher ;
       rdfs:comment "relation from a Product to a studio"@en ;
       rdfs:label "DevBy"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Love
:Love rdf:type owl:ObjectProperty ;
      owl:inverseOf :isLovedBy ;
      rdfs:domain :Lover ;
      rdfs:range :Character ;
      rdfs:comment "relation from a lover to a character"@en ;
      rdfs:label "loves"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/ReciprocallyLoves
:ReciprocallyLoves rdf:type owl:ObjectProperty ,
                            owl:SymmetricProperty ;
                   rdfs:domain :Character ;
                   rdfs:range :Character ;
                   rdfs:comment "relation from a character to a character"@en ;
                   rdfs:label "ReciprocallyLoves"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/SeriesDevBy
:SeriesDevBy rdf:type owl:ObjectProperty ;
             rdfs:domain :Series ;
             rdfs:range :DevPublisher ;
             owl:propertyChainAxiom ( <http://www.ontologydesignpatterns.org/cp/owl/bag.owl#hasItem>
                                      :DevBy
                                    ) ;
             rdfs:comment "relation from a saga to a studio"@en ;
             rdfs:label "SerieMadeBy"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/belongsTo
:belongsTo rdf:type owl:ObjectProperty ;
           owl:inverseOf :hasAsset ;
           rdfs:domain :Asset ;
           rdfs:range :Character ;
           rdfs:comment "relation from a asset to a character"@en ;
           rdfs:label "belongsTo"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/hasAsset
:hasAsset rdf:type owl:ObjectProperty ;
          rdfs:domain :Character ;
          rdfs:range :Asset ;
          rdfs:comment "relation from a character to an asset"@en ;
          rdfs:label "hasAsset"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/hasCharacter
:hasCharacter rdf:type owl:ObjectProperty ;
              rdfs:domain :Game ;
              rdfs:range :Character ;
              rdfs:comment "relation from a game to a character"@en ;
              rdfs:label "hasCharacter"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/hasDLC
:hasDLC rdf:type owl:ObjectProperty ;
        rdfs:domain :Game ;
        rdfs:range :DLC ;
        rdfs:comment "relation from a game to a dlc"@en ;
        rdfs:label "hasDLC"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/hasMap
:hasMap rdf:type owl:ObjectProperty ;
        owl:inverseOf :isMapOf ;
        rdfs:domain :Game ;
        rdfs:range :Map ;
        rdfs:comment "relation from a game to a map"@en ;
        rdfs:label "hasMap"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/hasProtagonist
:hasProtagonist rdf:type owl:ObjectProperty ;
                owl:inverseOf :isProtagonistOf ;
                rdfs:domain :Game ;
                rdfs:range :Character ;
                rdfs:comment "relation from a game to a character" ;
                rdfs:label "hasProtagonist"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/hasSex
:hasSex rdf:type owl:ObjectProperty ;
        rdfs:domain :Character ;
        rdfs:range :Gender ;
        rdfs:comment "relation from a character to a gender"@en ;
        rdfs:label "hasSex"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/hasTheme
:hasTheme rdf:type owl:ObjectProperty ;
          owl:inverseOf :isThemeOf ;
          rdfs:domain :Game ;
          rdfs:range :Themes ;
          rdfs:comment "Associates a story-driven game with one or more themes"@en ;
          rdfs:label "hasTheme"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/hasVillager
:hasVillager rdf:type owl:ObjectProperty ;
             owl:inverseOf :isVillagerOf ;
             rdfs:domain :Game ;
             rdfs:range :Character ;
             rdfs:comment "hasVillager - relationship between character and game"@en ;
             rdfs:label "hasVillager"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/hasWarrior
:hasWarrior rdf:type owl:ObjectProperty ;
            owl:inverseOf :isWarriorOf ;
            rdfs:domain :Game ;
            rdfs:range :Character ;
            rdfs:comment "A game has a warrior"@en ;
            rdfs:label "hasWarrior"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/isAssetOf
:isAssetOf rdf:type owl:ObjectProperty ;
           rdfs:domain :Asset ;
           rdfs:range :Game ;
           owl:propertyChainAxiom ( :belongsTo
                                    :AppearsIn
                                  ) ;
           rdfs:comment "relation from a asset to a game" ;
           rdfs:label "isAssetOf"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/isLovedBy
:isLovedBy rdf:type owl:ObjectProperty ;
           rdfs:domain :Character ;
           rdfs:range :Lover ;
           rdfs:comment "relation from a character to a lover"@en ;
           rdfs:label "isLovedBy"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/isMapOf
:isMapOf rdf:type owl:ObjectProperty ;
         rdfs:domain :Map ;
         rdfs:range :Game ;
         rdfs:comment "relation from a map to a game"@en ;
         rdfs:label "isMapOf"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/isProtagonistOf
:isProtagonistOf rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :AppearsIn ;
                 rdfs:domain :Character ;
                 rdfs:range :Game ;
                 rdfs:comment "relation from a character to a game"@en ;
                 rdfs:label "isProtagonistOf"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/isThemeOf
:isThemeOf rdf:type owl:ObjectProperty ;
           rdfs:domain :Themes ;
           rdfs:range :Game .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/isUsedBy
:isUsedBy rdf:type owl:ObjectProperty ;
          owl:inverseOf :usesWeapon ;
          rdfs:domain :Weapon ;
          rdfs:range :Character ;
          rdfs:comment "relation from a weapon to a character"@en ;
          rdfs:label "isUsedBy"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/isVillagerOf
:isVillagerOf rdf:type owl:ObjectProperty ;
              rdfs:domain :Character ;
              rdfs:range :Game ;
              rdfs:comment "iisVillagerOf - relationship between character and game"@en ;
              rdfs:label "isVillagerOf"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/isWarriorOf
:isWarriorOf rdf:type owl:ObjectProperty ;
             rdfs:domain :Character ;
             rdfs:range :Game ;
             rdfs:comment "A character is warrior of a game"@en ;
             rdfs:label "isWarriorOf"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/killBy
:killBy rdf:type owl:ObjectProperty ;
        owl:inverseOf :kills ;
        rdfs:domain :Character ;
        rdfs:range :Killer ;
        rdfs:comment "relation from a character to a killer" ;
        rdfs:label "KilledBy"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/kills
:kills rdf:type owl:ObjectProperty ;
       rdfs:domain :Killer ;
       rdfs:range :Character ;
       rdfs:comment "Relation from a killer to a character"@en ;
       rdfs:label "kills"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/makes
:makes rdf:type owl:ObjectProperty ;
       rdfs:domain :DevPublisher ;
       rdfs:range <http://schema.org/Product> ;
       rdfs:comment "A developer/publisher makes a product"@en ;
       rdfs:label "makes"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/usesWeapon
:usesWeapon rdf:type owl:ObjectProperty ;
            rdfs:domain :Character ;
            rdfs:range :Weapon ;
            rdfs:comment "relation from a character to a weapon" ;
            rdfs:label "usesWeapon"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Age
:Age rdf:type owl:DatatypeProperty ;
     rdfs:domain :Character ;
     rdfs:range xsd:int ;
     rdfs:comment "The number of years a character has existed for."@en ;
     rdfs:label "Age"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/MultiPlayerMode
:MultiPlayerMode rdf:type owl:DatatypeProperty ;
                 rdfs:domain :Game ;
                 rdfs:range xsd:boolean ;
                 rdfs:comment "Indicates whether a game supports multiplayer. This property is used to specify whether a game is designed to be played by multiple users simultaneously, either cooperatively or competitively, either locally or online."@en ;
                 rdfs:label "MultiPlayerMode"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Name
:Name rdf:type owl:DatatypeProperty ;
      rdfs:domain owl:Thing ;
      rdfs:range rdfs:Literal ;
      rdfs:comment "The name of something"@en ;
      rdfs:label "Name"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Sex
:Sex rdf:type owl:DatatypeProperty ;
     rdfs:domain :Character ;
     rdfs:range xsd:string ;
     rdfs:comment "The gender of a character"@en ;
     rdfs:label "Sex"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/SinglePlayerMode
:SinglePlayerMode rdf:type owl:DatatypeProperty ;
                  rdfs:domain :Game ;
                  rdfs:range xsd:boolean ;
                  rdfs:comment "Indicates whether a game supports a single-player mode. This property is used to specify whether a game is designed to be played by a single user, with an experience focused on narrative, personal challenge, or individual exploration." ;
                  rdfs:label "SinglePlayerMode"@en .


#################################################################
#    Classes
#################################################################

###  http://schema.org/Product
<http://schema.org/Product> rdf:type owl:Class ;
                            rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/list.owl#ListItem> ;
                            rdfs:comment "Represents something that can be sold"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Adult
:Adult rdf:type owl:Class ;
       rdfs:subClassOf :Character ;
       owl:disjointWith :Minor ;
       rdfs:comment "Character which age is over 18" ;
       rdfs:label "Adult"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Asset
:Asset rdf:type owl:Class ;
       rdfs:comment """Any asset that can be owned by a character or used in a game.
An important component of a video game"""@en ;
       rdfs:label "Asset"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Car
:Car rdf:type owl:Class ;
     rdfs:subClassOf :Asset ;
     rdfs:comment "A specific game asset that is used in games that represents a car"@en ;
     rdfs:label "Car"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Card
:Card rdf:type owl:Class ;
      rdfs:subClassOf :Asset ;
      rdfs:comment "A game asset that can be used in the context of a  game"@en ;
      rdfs:label "Card"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Card_Game
:Card_Game rdf:type owl:Class ;
           rdfs:subClassOf :Game ;
           rdfs:comment "A game based on cards"@en ;
           rdfs:label "Card_Game"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Character
:Character rdf:type owl:Class ;
           rdfs:comment "A character of a game" ;
           rdfs:label "Character"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Competitive_Game
:Competitive_Game rdf:type owl:Class ;
                  rdfs:subClassOf :Game ;
                  rdfs:comment "A videogame in which players compete to reach a goal." ;
                  rdfs:label "Competitive_Game"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/DLC
:DLC rdf:type owl:Class ;
     rdfs:subClassOf <http://schema.org/Product> ;
     rdfs:comment """Downloadable content, considered a Product.
Downloadable Content for a videogame. It generally is an expansion to the base version of the Game"""@en ;
     rdfs:label "DLC"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/DevPublisher
:DevPublisher rdf:type owl:Class ;
              rdfs:comment "A company that develops or publishes games" ;
              rdfs:label "DevPublisher"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/FemaleProtagonistGame
:FemaleProtagonistGame rdf:type owl:Class ;
                       owl:equivalentClass [ owl:intersectionOf ( :Story_Game
                                                                  [ rdf:type owl:Restriction ;
                                                                    owl:onProperty :hasProtagonist ;
                                                                    owl:someValuesFrom [ owl:intersectionOf ( :Character
                                                                                                              [ rdf:type owl:Restriction ;
                                                                                                                owl:onProperty :hasSex ;
                                                                                                                owl:hasValue :Female
                                                                                                              ]
                                                                                                            ) ;
                                                                                         rdf:type owl:Class
                                                                                       ]
                                                                  ]
                                                                ) ;
                                             rdf:type owl:Class
                                           ] ;
                       rdfs:subClassOf :Story_Game ;
                       rdfs:comment "A story game in which the protagonist is of gender: Female" ;
                       rdfs:label "FemaleProtagonistGame"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Game
:Game rdf:type owl:Class ;
      rdfs:subClassOf <http://schema.org/Product> ;
      rdfs:comment "Some sort of video game"@en ;
      rdfs:label "Game"@en ;
      owl:equivalentClass "https://www.wikidata.org/wiki/Q7889" .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Gender
:Gender rdf:type owl:Class ;
        owl:equivalentClass [ rdf:type owl:Class ;
                              owl:oneOf ( :Female
                                          :Male
                                          :NotSpecified
                                        )
                            ] ;
        rdfs:comment """The gender of some characters: in the domain of videogames could be something other than male/female.
Enumerated class for Gender (Female, Male, NotSpecified)""" ;
        rdfs:label "Gender"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Killer
:Killer rdf:type owl:Class ;
        owl:equivalentClass [ owl:intersectionOf ( :Character
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :kills ;
                                                     owl:someValuesFrom :Character
                                                   ]
                                                 ) ;
                              rdf:type owl:Class
                            ] ;
        rdfs:subClassOf :Character ;
        rdfs:comment "A character that has killed some other character"@en ;
        rdfs:label "Killer"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Lover
:Lover rdf:type owl:Class ;
       owl:equivalentClass [ owl:intersectionOf ( :Character
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :Love ;
                                                    owl:someValuesFrom :Character
                                                  ]
                                                ) ;
                             rdf:type owl:Class
                           ] ;
       rdfs:subClassOf :Character ;
       rdfs:comment "A character that is in love with some other character" ;
       rdfs:label "Lover"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/MaleProtagonistGame
:MaleProtagonistGame rdf:type owl:Class ;
                     owl:equivalentClass [ owl:intersectionOf ( :Story_Game
                                                                [ rdf:type owl:Restriction ;
                                                                  owl:onProperty :hasProtagonist ;
                                                                  owl:someValuesFrom [ owl:intersectionOf ( :Character
                                                                                                            [ rdf:type owl:Restriction ;
                                                                                                              owl:onProperty :hasSex ;
                                                                                                              owl:hasValue :Male
                                                                                                            ]
                                                                                                          ) ;
                                                                                       rdf:type owl:Class
                                                                                     ]
                                                                ]
                                                              ) ;
                                           rdf:type owl:Class
                                         ] ;
                     rdfs:subClassOf :Story_Game ;
                     rdfs:comment "A story game in which the protagonist is of gender: Male"@en ;
                     rdfs:label "MaleProtagonistGame"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Map
:Map rdf:type owl:Class ;
     rdfs:comment "A fictionary representation of either a real or an imaginary place in which the action is set during a game" ;
     rdfs:label "Map"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Minor
:Minor rdf:type owl:Class ;
       rdfs:subClassOf :Character ;
       rdfs:comment "Character which age is under 18"@en ;
       rdfs:label "Minor"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/MultiPlayerGame
:MultiPlayerGame rdf:type owl:Class ;
                 rdfs:subClassOf :Game ;
                 rdfs:comment "Video game that involves multiple players simultaneously, in cooperative or competitive mode, locally or online."@en ;
                 rdfs:label "MultiPlayerGame"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Protagonist
:Protagonist rdf:type owl:Class ;
             owl:equivalentClass [ owl:intersectionOf ( :Character
                                                        [ rdf:type owl:Restriction ;
                                                          owl:onProperty :isProtagonistOf ;
                                                          owl:someValuesFrom :Game
                                                        ]
                                                      ) ;
                                   rdf:type owl:Class
                                 ] ;
             rdfs:subClassOf :Character ;
             rdfs:comment """A character that is considered the leader / main character of a game.
Usually the  one the player plays as.""" ;
             rdfs:label "Protagonist"@en .


###  http://www.semanticweb.org/loren_16pbl1h/ontologies/2025/0/Games_Ontology/Series
:Series rdf:type owl:Class ;
        rdfs:subClassOf <http://www.ontologydesignpatterns.org/cp/owl/list.owl#List> ;
