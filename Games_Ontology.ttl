#################################################################
#   GENERAL AXIOMS
#################################################################

[ owl:intersectionOf ( :Character
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :isProtagonistOf ;
                         owl:someValuesFrom :Game
                       ]
                     ) ;
  rdf:type owl:Class ;
  rdfs:subClassOf :Character
] .

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Car
                :Card
                :Weapon
              )
] .

#################################################################
#   RULES (SWRL)
#################################################################

:c rdf:type <http://www.w3.org/2003/11/swrl#Variable> .
:cage rdf:type <http://www.w3.org/2003/11/swrl#Variable> .

[ <http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> "true"^^xsd:boolean ;
  rdfs:comment "A character is an Adult if Age >= 18"@en ;
  rdfs:label "AdultRule"@en ;
  rdf:type <http://www.w3.org/2003/11/swrl#Imp> ;
  <http://www.w3.org/2003/11/swrl#body>
    [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
      rdf:first 
        [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
          <http://www.w3.org/2003/11/swrl#classPredicate> :Character ;
          <http://www.w3.org/2003/11/swrl#argument1> :c
        ] ;
      rdf:rest 
        [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
          rdf:first 
            [ rdf:type <http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom> ;
              <http://www.w3.org/2003/11/swrl#propertyPredicate> :Age ;
              <http://www.w3.org/2003/11/swrl#argument1> :c ;
              <http://www.w3.org/2003/11/swrl#argument2> :cage
            ] ;
          rdf:rest 
            [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
              rdf:first
                [ rdf:type <http://www.w3.org/2003/11/swrl#BuiltinAtom> ;
                  <http://www.w3.org/2003/11/swrl#builtin> <http://www.w3.org/2003/11/swrlb#greaterThanOrEqual> ;
                  <http://www.w3.org/2003/11/swrl#arguments>
                    [ rdf:type rdf:List ;
                      rdf:first :cage ;
                      rdf:rest 
                        [ rdf:type rdf:List ;
                          rdf:first 18 ;
                          rdf:rest rdf:nil
                        ]
                    ]
                ] ;
              rdf:rest rdf:nil
            ]
        ]
    ] ;
  <http://www.w3.org/2003/11/swrl#head>
    [ rdf:type <http://www.w3.org/2003/11/swrl#AtomList> ;
      rdf:first 
        [ rdf:type <http://www.w3.org/2003/11/swrl#ClassAtom> ;
          <http://www.w3.org/2003/11/swrl#classPredicate> :Adult ;
          <http://www.w3.org/2003/11/swrl#argument1> :c
        ] ;
      rdf:rest rdf:nil
    ]
] .
